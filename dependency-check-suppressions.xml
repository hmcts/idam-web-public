<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">

    <!-- This CVE is about JSTL in conjunction with XML/XSLT tags, we do not use these tags -->
    <suppress>
        <notes><![CDATA[
   file name: jstl-1.2.jar
   ]]></notes>
        <gav regex="true">^.*$</gav>
        <cve>CVE-2015-0254</cve>
    </suppress>

    <!-- False positive. See https://github.com/jeremylong/DependencyCheck/issues/1827 -->
    <suppress>
        <gav regex="true">^org\.springframework\.security:spring-security.*$</gav>
        <cve>CVE-2018-1258</cve>
    </suppress>

    <!-- False positive. See https://nvd.nist.gov/vuln/detail/CVE-2020-23171
         This application does not use lang-tag to write files to directories.
    -->
    <suppress>
        <gav regex="true">^com\.nimbusds:lang-tag.*$</gav>
        <cve>CVE-2020-23171</cve>
    </suppress>

    <!-- Fixed in an upcoming release -->
    <suppress>
        <gav regex="true">^org\.apache\.tomcat\.embed:.*$</gav>
        <cve>CVE-2021-42340</cve>
    </suppress>

</suppressions>
