<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.1.xsd">

    <!-- This CVE is about JSTL in conjunction with XML/XSLT tags, we do not use these tags -->
    <suppress>
        <notes><![CDATA[
   file name: jstl-1.2.jar
   ]]></notes>
        <gav regex="true">^.*$</gav>
        <cve>CVE-2015-0254</cve>
    </suppress>

    <!-- These are false positive CVEs from the dependency check plugin -->
    <suppress>
        <gav regex="true">^.*spring-.*$</gav>
        <cve>CVE-2016-1000027</cve>
        <cve>CVE-2020-5408</cve>
    </suppress>

    <!-- NVD data is incorrect. They have the CPE as cpe:2.3:a:netty:netty:4.1.86:*:*:*:*:*:*:*.
    An appropriate suppression until it's fixed -->
    <suppress>
        <packageUrl regex="true">^pkg:maven/io\.netty/.*@4\.1\.86\.Final$</packageUrl>
        <cve>CVE-2022-41915</cve>
    </suppress>

</suppressions>
